
    let Foldable = ../Foldable/Type

in  let fold = ../Prelude/Optional/fold

in  (   { fold =
              λ(a : Type)
            → λ(ts : Optional a)
            → λ(b : Type)
            → λ(f : a → b → b)
            → λ(z : b)
            → fold a ts b (λ(x : a) → f x z) z
        }
      : Foldable Optional
    )
