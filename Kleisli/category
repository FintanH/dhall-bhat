  λ(m : Type → Type)
→ λ(monad : ./../Monad/Type m)
→   { id =
        monad.pure
    , compose =
          λ(a : Type)
        → λ(b : Type)
        → λ(c : Type)
        → λ(g : b → m c)
        → λ(f : a → m b)
        → λ(x : a)
        → monad.bind b c (f x) g
    }
  : ./../Category/type (./type m)
