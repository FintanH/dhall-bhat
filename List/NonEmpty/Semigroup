    let NonEmpty = ./Type

in  let Semigroup = ./../../Semigroup/Type

in    λ(a : Type)
    →     let List/op = (./../Semigroup a).op
      
      in    { op =
                  λ(x : NonEmpty a)
                → λ(y : NonEmpty a)
                → { head = x.head, tail = List/op x.tail (./toList a y) }
            }
          : Semigroup (NonEmpty a)
