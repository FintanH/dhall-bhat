    let Applicative = ./../Applicative/Type

in  let Either = ./../Either/Type

in    λ(f : Type → Type)
    → λ(c : ./Type f)
    → λ(applicative : Applicative f)
    → λ(a : Type)
    → λ(b : Type)
    → λ(k : f (a → Either a b))
    → λ(fa : f a)
    → c.separate a b (applicative.ap a (Either a b) k fa)
