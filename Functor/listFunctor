    let Functor = ./Type

in  let fold =
          https://ipfs.io/ipfs/QmQ8w5PLcsNz56dMvRtq54vbuPe9cNnCCUXAQp6xLc6Ccx/Prelude/List/fold

in  (   { map =
              λ(a : Type)
            → λ(b : Type)
            → λ(g : a → b)
            → λ(list : List a)
            → fold
              a
              list
              (List b)
              (λ(x : a) → λ(xs : List b) → [ g x ] # xs)
              ([] : List b)
        }
      : Functor  List
    )
